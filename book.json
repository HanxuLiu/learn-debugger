{
    "plugins": [
        "back-to-top-button",
        "hide-element",
        "chapter-fold",
        "expandable-chapters",
        "code",
        "splitter",
        "-lunr",
        "-search",
        "search-pro",
        "insert-logo",
        "custom-favicon",
        "tbfed-pagefooter",
        "-sharing",
        "page-treeview",
        "popup"
    ],
    "pluginsConfig": {
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "insert-logo": {
            "url": "https://cdn.jsdelivr.net/gh/HanxuLiu/CDN1/img/2021/202203061424792.png",
            "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "favicon": "./book.ico",
        "tbfed-pagefooter": {
            "copyright": "",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "page-treeview": {
            "copyright": "",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        }
    }
}
